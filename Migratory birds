#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t,n,k,i,j,s=0;
    cin>>t;
    
    for(i=0;i<t;i++)
    {
        int t2;
        cin>>n>>k;
        t2=n%k;
        if(t2%2==0)
        {
            cout<<"YES"<<endl;
        }
        else
        {
            cout<<"NO"<<endl;
        }
        
    }
    
    return 0;
    
}
