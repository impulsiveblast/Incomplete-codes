#include<iostream>
using namespace std;

int main()
{
    int a=0,b=0,c=0,d=0,e=0;
    int n,i,temp;
    cin>>n;
    int p[n];
    for(i=0;i<n;i++)
    {
        cin>>p[i];
    }
    
    for(i=0;i<n;i++)
    {
        if(p[i]==1)
        {
            a++;
        }
        
        
        else if(p[i]==2)
        {
            b++;
        }
        
        else if(p[i]==3)
        {
            c++;
        }
        
        else if(p[i]==4)
        {
            d++;
        }
        
        else
        {
            e++;
        }
    }
    
    int q[5],j;
    
    q[0]=a;
    q[1]=b;
    q[2]=c;
    q[3]=d;
    q[4]=e;
    
    
    for(i=0;i<4;i++)
    {
        for(j=0;j<5-i;j++)
        {
            if(q[j]>q[j+1])
            {
                int temp=q[j];
                q[j]=q[j+1];
                q[j+1]=temp;
            }
        }
    }
    
    if(q[4]==a)
    {
    cout<<"1";
        
    }
    else if(q[4]==b)
    {
        cout<<"2";
        
    }
    else if(q[4]==c)
    {
        cout<<"3";
    }
    else if(q[4]==d)
    {
        cout<<"4";
    }
    else
    {
        cout<<"5";
    
    }
    
    return 0;
    
    
}
