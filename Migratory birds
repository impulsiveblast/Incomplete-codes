#include<iostream>
using namespace std;

int main()
{
    int n,i,j,m=0;
    
    cin>>n;
    
    int a[n];
    
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    
    int b[5];
    
  for(i=0;i<5;i++)
  {
      b[i]=0;
  }
    
    for(i=0;i<5;i++)
    {
        for(j=0;j<n;j++)
     {
            if(a[j]==i+1)
        {
            b[i]++;
        }
     }
    
        
    }
    
    for(i=0;i<5;i++)
    {
        if(b[i]>0)
        {
            m++;
        }
    }
    
    cout<<m;
    
    return 0;
}
    
    
