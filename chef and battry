#include<bits/stdc++.h>
using namespace std;

int main()
{
   int n1,n2,i,j;
   cin>>n1;
   
   for(i=0;i<n1;i++)
   {
       cin>>n2;
       
       if((n2-50)>0)
       {
             if((n2-50)%3==0)
             {
                 cout<<(n2-50)/3;
             }
             else
             {
                if((n2-50)%3==2)
                {
                    cout<<((n2-50)/3)+4<<endl;
                }
                else
                {
                    cout<<((n2-50)/3)+2<<endl;
                }
                
                
             }
       }
       
       else
       {
           int t=(50-n2),sum=0;
           
           if(t%2==0)
           {
               cout<<(t/2)<<endl;
           }
           else
           {
               sum+=t/2;
               cout<<sum+3<<endl;
               
           }
           
       }
       
   }
   
   
   return 0;

}
