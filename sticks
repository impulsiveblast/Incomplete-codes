#include<iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    
    for(i=0;i<n-1;i++)
    {
        for(j=0;j<n-i;j++)
        {
            t=a[i];
            if(a[i]>a[i+1])
            {
                a[i+1]=a[i];
                a[i+1]=t;
            }
        }
        
    }
    
    for(i=0;i<n;i++)
    {
        if(a[i]>a[i+1])
        {
            a[i]--;
        }
        
    }
}
