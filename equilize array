#include<ostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int a[n];
    
    int i;
    
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    
    for(i=0;i<n-1;i++)
    {
        for(j=0;j<n-i;j++)
        {
            if(a[j]>a[j+1])
            t=a[j];
            a[j]=a[j+1];
            a[j+1]=t;
        }
    }
    
    int b[a[n-1]+1];
    
    for(i=0;i<a[n-1]+1;i++)
    {
       b[i]=i; 
    }
    
    
    return 0;
}
