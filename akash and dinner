#include<iostream>
using namespace std;

int main()

{
    int i,a,j,b,n,k;
    cin>>n;
    
    for(i=0;i<n;i++)
    {
        int t;
        cin>>a>>b;
        
        int c0[a],c1[a];
        
        for(i=0;i<a;i++)
        {
           cin>>c0[i]; 
        }
        
        for(i=0;i<a;i++)
        {
           cin>>c1[i]; 
        }
        
        for(i=0;i<a;i++)
        {
           t=max(c0[i],c0[i+1]); 
        }
        
        int c2[t];
        j=0;
        while(j<t)
        {
            int m=0;
            for(k=0;k<n;k++)
          {
           if( c0[k]==j+1)
            {
               if(m==0)
                {
                   c2[j]=c1[k];
                   m++;
                }
                
                else
                {
                    c2[j]=min(c2[j],c1[k]);
                }
               
             }
            
           }
           
           j++;
        }
        
        for(i=0;i<n;i++)
    {
        cout<<c2[i]<<endl;
    }
        
        
        
    }

    return 0;

}
