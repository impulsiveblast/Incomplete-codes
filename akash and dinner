#include<iostream>
using namespace std;

int main()

{
    int i,a,j,b,n;
    cin>>n;
    
    for(i=0;i<n;i++)
    {
        cin>>a>>b;
        
        int c0[a],c1[a]
        
        for(j=0;j<a;j++)
        {
            cin>>c0[j];
        }
        
        for(j=0;j<a;j++)
        {
            cin>>c1[j];
        }
        
        for(j=0;j<a;j++)
        {
            t=max(c0[j],c0[j+1]);
        }
        
        int c2[t];
        k=1;
        while(k<t+1)
        {
            s=0;
            for(j=0;j<t;j++)
          {
            if(c0[j]==k)
             {
               if(s==0)
               {
                   c2[k-1]=c1[j];
               }
               else
               {
                   c2[k-1]=min(c2[k-1],c1[j]);
               }
             }
           
             k++;
           
           }
          
        }
        
        
        
        
        
        
        
    }
}
