             n1++;
         }
         
         else if(s[i]=='o')
         {
             o1++;
         }
         
         else if(s[i]=='p')
         {
             p1++;
         }
         
         else if(s[i]=='q')
         {
             q1++;
         }
         
         else if(s[i]=='r')
         {
             r1++;
         }
         
         else if(s[i]=='s')
         {
             s1++;
