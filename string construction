#include<iostream>
#include<string>
using namespace std;

int main()
{
    int n,i=0,j,m=0;
    cin>>n;
    
    int a2[n];
    string s;
    
    int a1=0,b1=0,c1=0,d1=0,e1=0,f1=0,g1=0,h1=0,i1=0,j1=0,k1=0,l1=0,m1=0,n1=0,o1=0,p1=0,q1=0,r1=0,s1=0,t1=0,u1=0,v1=0,w1=0,x1=0,y1=0,z1=0;
    for(j=0;j<n;j++)
   {
     
    m=0;
     cin>>s;
     
     while(s[i] != '\0')
     {
         if(s[i]=='a')
         {
             a1++;
         }
         
         else if(s[i]=='b')
         {
             b1++;
         }
         
         else if(s[i]=='c')
         {
             c1++;
         }
         
         else if(s[i]=='d')
