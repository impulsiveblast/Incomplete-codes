         else if(s[i]=='w')
         {
             w1++;
         }
         
         else if(s[i]=='x')
         {
             x1++;
         }
         
         else if(s[i]=='y')
         {
             y1++;
         }
         
         else if(s[i]=='z')
         {
             z1++;
         }
         
         else 
         {
             a1+=0;
         }
         
         i++;
         
         
      
      }
      
      while(a1>0)
      {
          m++;
          a1-=a1;
      }
      
       while(b1>0)
      {
          m++;
          b1-=b1;
      }
      
       while(c1>0)
      {
          m++;
          c1-=c1;
      }
      
       while(d1>0)
      {
          m++;
