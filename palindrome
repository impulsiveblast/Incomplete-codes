#include<iostream>
using namespace std;

int main()
{
    int n,i;
    cin>>n;
    char a[n+1];
    int b=1;
    
    cin>>a;
    
    for(i=0;i<n;i++)
    {
        if(a[i] != a[n-1-i])
        {
            b=0;
        }
    }
    
    if(b==1)
    
        cout<<"word is a palindrome";
    
    
    else
    
        cout<<"word is not a palindrome";
    
    
    
    return 0;
    
    
}
