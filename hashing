#include<bits/stdc++.h>
using namespace std;

#define N=1e7+10;

int hsh[N];
int main()
 {                                     //time _Complexity O(N) Space _Complexity O(N)
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
      cin>>a[i];
      hash[a[i]]++;
    }
    int q;
    cin>>q;
    while(q--)
    {
        int x;
        cin>>x;
        cout<<hash[x]<endl;
    }
    
    return 0;
 }
