#include<iostream>
#include<algorithm>

using namespace std;

int main()
{
    int i,n1,n2;
    
    cin>>n1>>n2;
    
    int a[n2],c[n1];
    
    for(i=0;i<n2;i++)
    {
        cin>>a[i];
    }
    
    
    for(i=0;i<n1;i++)
    {
        for(j=0;j<n2;j++)
        {
            if(i==a[j])
            {
                c[i]=0;
            }
        }
        
        c[]
        
        
        
    }
    
    
}
