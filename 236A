
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int i=0;
    string s;
    cin>>s;
    
    set<char> s1;
    
    while(s[i]!='\0')
    {
        s1.insert(s[i]);
        i++;
    }
    
    if((s1.size())%2==0)
    {
        cout<<"CHAT WITH HER!";
    }
    else
    {
        cout<<"IGNORE HIM!";
    }
    
    
    return 0;
}
