#include <bits/stdc++.h>
using namespace std;

int main()
{
      int n;
      cin>>n;
      char x;
      int a,b;
      for(int i=0;i<n;i++)
      {
          cin>>a>>b;
          vector<char> v1,v2;
          a1=0;
          b1=0;
          for(j=0;j<a;j++)
          {
              cin>>x;
              v1.push_back(x);
          }
          
          for(j=0;j<b;j++)
          {
              cin>>x;
              v2.push_back(x);
          }
          
          for(j=0;j<a-1;j++)
          {
              if(v1[j]==v1[j+1])
              {
                  a1=1;
              }
          }
          
          for(j=0;j<b-1;j++)
          {
              if(v2[j]==v2[j+1])
              {
                  b1=1;
              }
          }
          
          if(a1==1 && b1==1)
          {
              if(v1[size()-1]==v2[size()-1])
              {
                cout<<"NO"<<endl;   
              }
              else
              {
                  
              }
          }
          
          
      }
     
    return 0;
}
